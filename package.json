{
  "name": "create-exponent-app",
  "description": "A basic Exponent application boilerplate.",
  "version": "0.0.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "exponent": "12.0.5",
    "mobx": "2.7.0",
    "mobx-react": "4.0.4",
    "react": "15.3.2",
    "react-native": "git+https://github.com/exponentjs/react-native#sdk-12.0.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-transform-flow-strip-types": "6.21.0",
    "babel-preset-react-native-stage-0": "1.0.1",
    "enzyme": "2.7.0",
    "enzyme-to-json": "1.4.5",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "flow-bin": "0.37.4",
    "jest": "18.0.0",
    "jest-exponent": "0.2.0",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2"
  },
  "scripts": {
    "start": "exp start --dev --lan && exp ios #&& exp android",
    "test": "jest -u #--coverage",
    "build": "exp build:ios #&& exp build:android",
    "lint": "eslint .; exit 0",
    "flow": "flow --show-all-errors; exit 0",
    "outdated": "yarn outdated && npm outdated -g; exit 0",
    "brew": "brew update && brew upgrade",
    "clean": "find . -name .DS_Store -type f -delete -print"
  },
  "jest": {
    "preset": "jest-exponent"
  }
}
