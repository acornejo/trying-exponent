{
  "name": "create-exponent-app",
  "description": "A basic Exponent application boilerplate.",
  "version": "0.0.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "exponent": "11.0.2",
    "mobx": "2.6.2",
    "mobx-react": "3.5.9",
    "react": "15.3.2",
    "react-native": "git+https://github.com/exponentjs/react-native#sdk-11.0.3"
  },
  "devDependencies": {
    "babel-eslint": "7.1.0",
    "babel-jest": "17.0.0",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-preset-react-native-stage-0": "1.0.1",
    "enzyme": "2.6.0",
    "eslint": "3.10.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.6.0",
    "exp": "0.27.1",
    "flow-bin": "0.35.0",
    "jest": "17.0.0",
    "jest-exponent": "0.1.3",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2"
  },
  "scripts": {
    "start": "exp stop --all && exp start --dev --lan && exp ios #&& exp android",
    "test": "jest #--coverage",
    "build": "exp build:ios #&& exp build:android",
    "lint": "eslint .; exit 0",
    "flow": "flow --show-all-errors; exit 0"
  },
  "jest": {
    "preset": "jest-exponent"
  }
}
