{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": [1, "always"],
    "class-methods-use-this": 0,
    "comma-dangle": [1, "never"],
    "indent": [1, 2],
    "jsx-quotes": [1, "prefer-single"],
    "linebreak-style": [1, "unix"],
    "no-alert": 0,
    "no-console": 0,
    "no-mixed-spaces-and-tabs": 1,
    "no-return-assign": 0,
    "no-unused-vars": 1,
    "quotes": [1, "single"],
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "semi": [1, "always"],
    "no-use-before-define": 0
  }
}
